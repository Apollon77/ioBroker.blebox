{
    "i18n": true,
    "type": "panel",
    "items": {
        "extLog": {
            "type": "checkbox",
            "label": "Extended logging",
            "newLine": true,
            "size": 2,
            "xs": 12,
            "sm": 12,
            "md": 12,
            "lg": 12,
            "xl": 12
        },
        "divider": {
            "type": "divider"
        },
        "devices": {
            "type": "table",
            "label": "Devices",
            "size": 2,
            "xs": 12,
            "sm": 12,
            "md": 12,
            "lg": 12,
            "xl": 12,
            "items": [
                {
                    "type": "text",
                    "width": "25%",
                    "title": {
                        "en": "Name"
                    },
                    "attr": "dev_name",
                    "filter": false,
                    "sort": true,
                    "default": ""
                },
                {
                    "type": "text",
                    "width": "20%",
                    "title": {
                        "en": "Smart name"
                    },
                    "attr": "smart_name",
                    "filter": false,
                    "sort": true,
                    "default": ""
                },
                {
                    "type": "text",
                    "width": "20%",
                    "title": {
                        "en": "IP Adress"
                    },
                    "attr": "dev_ip",
                    "filter": false,
                    "sort": true,
                    "default": ""
                },
                {
                    "type": "number",
                    "width": "10%",
                    "title": {
                        "en": "Port"
                    },
                    "attr": "dev_port",
                    "filter": false,
                    "sort": true,
                    "default": "80"
                },
                {
                    "type": "number",
                    "width": "10%",
                    "title": {
                        "en": "Update period"
                    },
                    "attr": "polling",
                    "filter": false,
                    "sort": true,
                    "default": "360"
                },
                {
                    "type": "select",
                    "width": "15%",
                    "title": {
                        "en": "Type"
                    },
                    "attr": "dev_type",
                    "filter": false,
                    "sort": true,
                    "options": [
                        {
                            "label": "gatebox",
                            "value": "gatebox"
                        },
                        {
                            "label": "multisensor [beta]",
                            "value": "multisensor"
                        },
                        {
                            "label": "tempSensor PRO [beta]",
                            "value": "tempSensor_PRO"
                        },
                        {
                            "label": "tempSensorAC [beta]",
                            "value": "tempSensorAC"
                        },
                        {
                            "label": "humiditySensor [beta]",
                            "value": "humiditySensor"
                        },
                        {
                            "label": "windSensor PRO [beta]",
                            "value": "windSensor_PRO"
                        },
                        {
                            "label": "rainSensor [beta]",
                            "value": "rainSensor"
                        },
                        {
                            "label": "floodSensor [beta]",
                            "value": "floodSensor"
                        },
                        {
                            "label": "saunabox [beta]",
                            "value": "saunabox"
                        },
                        {
                            "label": "shutterbox",
                            "value": "shutterbox"
                        },
                        {
                            "label": "shutterboxDC [beta]",
                            "value": "shutterboxDC"
                        },
                        {
                            "label": "shutterboxDIN [beta]",
                            "value": "shutterboxDIN"
                        },
                        {
                            "label": "switchbox",
                            "value": "switchbox"
                        },
                        {
                            "label": "switchBoxD [beta]",
                            "value": "switchBoxD"
                        },
                        {
                            "label": "switchBoxDC [beta]",
                            "value": "switchBoxDC"
                        },
                        {
                            "label": "switchBox DIN [beta]",
                            "value": "switchBox_DIN"
                        },
                        {
                            "label": "switchBoxD DIN [beta]",
                            "value": "switchBoxD_DIN"
                        },
                        {
                            "label": "switchBoxT PRO [beta]",
                            "value": "switchBoxT_PRO"
                        },
                        {
                            "label": "tempsensor [beta]",
                            "value": "tempsensor"
                        },
                        {
                            "label": "tvlift [beta]",
                            "value": "tvlift"
                        }
                    ]
                }
            ]
        }
    }
}